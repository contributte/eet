{
  "name": "contributte/eet",
  "description": "EET integration into Nette",
  "keywords": [
    "Electronic records of sale",
    "Nette EET"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/contributte/eet",
  "authors": [
    {
      "name": "Filip Šedivý",
      "homepage": "https://filipsedivy.cz"
    },
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "filipsedivy/php-eet": "^4.1",
    "nette/di": "^3.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\EET\\": "src/Contributte/EET"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
